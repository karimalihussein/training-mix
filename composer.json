{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "algolia/algoliasearch-client-php": "^3.3",
        "beyondcode/laravel-websockets": "^1.14",
        "codezero/laravel-localized-routes": "^2.9",
        "devinweb/laravel-hyperpay": "^1.1",
        "ebess/advanced-nova-media-library": "^4.0",
        "gender-api/client": "^1.0",
        "guzzlehttp/guzzle": "^7.5",
        "http-interop/http-factory-guzzle": "^1.0",
        "itsgoingd/clockwork": "^5.1",
        "laravel-shift/blueprint": "^2.1",
        "laravel/fortify": "^1.16",
        "laravel/framework": "^9.2",
        "laravel/helpers": "^1.5",
        "laravel/nova": "~4.0",
        "laravel/sanctum": "^2.14.1",
        "laravel/scout": "^9.8",
        "laravel/socialite": "^5.5",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.7",
        "league/omnipay": "^3",
        "livewire/livewire": "^2.12",
        "lorisleiva/laravel-actions": "^2.4",
        "meilisearch/meilisearch-php": "^0.24.2",
        "mjaschen/phpgeo": "^4.2",
        "mll-lab/laravel-graphiql": "^1.2",
        "netflie/whatsapp-cloud-api": "^1.2",
        "nuwave/lighthouse": "^6.5",
        "nwidart/laravel-modules": "^9.0",
        "omnipay/paypal": "^3.0",
        "opcodesio/log-viewer": "^1.7",
        "phpoffice/phpspreadsheet": "^1.23",
        "predis/predis": "^2.0",
        "pusher/pusher-php-server": "^7.2",
        "qoraiche/laravel-mail-editor": "^4.0",
        "roach-php/core": "^1.1",
        "roach-php/laravel": "^1.0",
        "sammyjo20/saloon": "^2.0",
        "schulzefelix/laravel-search-console": "^1.8",
        "sentry/sentry-laravel": "^2.13",
        "spatie/laravel-data": "^2.1",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-query-builder": "^5.2",
        "spatie/laravel-settings": "^2.4",
        "spatie/valuestore": "^1.3",
        "stevebauman/location": "^6.5",
        "symfony/finder": "^6.2",
        "tailflow/laravel-orion": "^2.19",
        "thejano/laravel-domain-driven-design": "^1.0",
        "torann/geoip": "^3.0",
        "torann/laravel-repository": "^0.6.2",
        "twilio/sdk": "^6.37",
        "vyuldashev/nova-permission": "^3.2",
        "weidner/goutte": "^2.2",
        "xaboy/form-builder": "~2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "dietercoopman/laravel-showsql": "^1.4",
        "fakerphp/faker": "^1.9.1",
        "jasonmccreary/laravel-test-assertions": "^2.2",
        "laravel/breeze": "^1.9",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "mtolhuys/laravel-schematics": "dev-master",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.1",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php" 
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "0.0.0/composer-include-files": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        "demo": {
            "type": "path",
            "url": "../saasforces/demo"
        }
    }
}
